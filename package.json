{
  "name": "flowgives-crowdfunding",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "db:setup": "tsx scripts/setup-database.ts",
    "db:push": "supabase db push",
    "db:pull": "supabase db pull",
    "db:reset": "supabase db reset",
    "db:migration:list": "supabase migration list",
    "db:migration:new": "supabase migration new",
    "storage:setup": "tsx scripts/setup-storage.ts",
    "add-image-column": "tsx scripts/add-image-column.ts",
    "add-images": "tsx scripts/add-image-column.ts && Invoke-RestMethod -Uri http://localhost:3001/api/projects/add-images -Method POST -ContentType application/json",
    "flow:deploy": "flow project deploy --network=testnet",
    "flow:deploy:emulator": "flow project deploy --network=emulator",
    "flow:test": "flow test",
    "flow:emulator": "flow emulator start"
  },
  "dependencies": {
    "@onflow/fcl": "^1.12.2",
    "@onflow/types": "^1.4.0",
    "@supabase/supabase-js": "^2.39.3",
    "next": "^14.2.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.18",
    "dotenv": "^17.2.3",
    "eslint": "^8",
    "eslint-config-next": "^14.2.0",
    "fast-check": "^3.15.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.35",
    "supabase": "^2.72.9",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.21.0",
    "typescript": "^5"
  }
